{
  "name": "wavespeedai-ai-provider",
  "version": "0.1.1",
  "description": "Unofficial Vercel AI Provider for WaveSpeedAI.",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "oxlint",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "prettier-write": "prettier . --write \"./**/*.ts*\"",
    "changeset": "changeset",
    "test": "bun run test:node && bun run test:edge",
    "test:node": "echo 'Running Node.js tests' && vitest --config vitest.node.config.js run",
    "test:edge": "echo 'Running Edge tests' && vitest --config vitest.edge.config.js run",
    "ci:version": "changeset version && bun install --no-frozen-lockfile",
    "ci:release": "bun clean && bun run build && changeset publish"
  },
  "dependencies": {
    "@ai-sdk/provider": "^3.0.2",
    "@ai-sdk/provider-utils": "^4.0.5",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@edge-runtime/vm": "^5.0.0",
    "@types/bun": "^1.3.6",
    "ai": "^6.0.33",
    "oxlint": "^1.39.0",
    "prettier": "^3.7.4",
    "tsdown": "^0.19.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "keywords": [
    "ai",
    "wavespeedai",
    "provider",
    "vercel",
    "vercel-ai-sdk",
    "ai-provider"
  ],
  "author": {
    "name": "Francisco Pizarro",
    "email": "francisco_p@outlook.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/dvaJi/wavespeedai-ai-provider",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvaJi/wavespeedai-ai-provider.git"
  },
  "bugs": {
    "url": "https://github.com/dvaJi/wavespeedai-ai-provider/issues"
  },
  "packageManager": "bun@1.3.6",
  "publishConfig": {
    "access": "public"
  }
}
