{
  "name": "unofficial-wavespeedai-ai-provider",
  "version": "0.0.2",
  "description": "Vercel AI Provider for WaveSpeedAI.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "oxlint",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "prettier-write": "prettier . --write \"./**/*.ts*\"",
    "changeset": "changeset",
    "test": "bun run test:node && bun run test:edge",
    "test:node": "echo 'Running Node.js tests' && vitest --config vitest.node.config.js run",
    "test:edge": "echo 'Running Edge tests' && vitest --config vitest.edge.config.js run",
    "ci:version": "changeset version && bun install --no-frozen-lockfile",
    "ci:release": "bun clean && bun run build && changeset publish"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.27",
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.17",
    "dotenv": "16.4.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@edge-runtime/vm": "^5.0.0",
    "@types/bun": "^1.3.3",
    "ai": "^5.0.102",
    "oxlint": "^1.30.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.7",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "keywords": [
    "ai",
    "wavespeedai",
    "provider",
    "vercel",
    "vercel-ai-sdk",
    "ai-provider"
  ],
  "author": {
    "name": "Francisco Pizarro",
    "email": "francisco_p@outlook.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/dvaJi/unofficial-wavespeedai-ai-provider",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvaJi/unofficial-wavespeedai-ai-provider.git"
  },
  "bugs": {
    "url": "https://github.com/dvaJi/unofficial-wavespeedai-ai-provider/issues"
  },
  "packageManager": "bun@1.3.3",
  "publishConfig": {
    "access": "public"
  }
}
